{
  "agents": {
    "defaults": {
      "workspace": "/workspace",
      "model": {
        "primary": "glm/glm-4.7",
        "fallbacks": ["ark/deepseek-v3.2"]
      },
      "models": {
        "glm/glm-4.7": { "alias": "glm" },
        "ark/deepseek-v3.2": { "alias": "ds" }
      },
      "skipBootstrap": false,
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      },
      "memorySearch": {
        "provider": "openai",
        "model": "embedding-3",
        "sources": ["memory"],
        "remote": {
          "baseUrl": "https://open.bigmodel.cn/api/paas/v4/"
        }
      }
    }
  },
  "models": {
    "providers": {}
  },
  "channels": {
    "feishu": {
      "enabled": true,
      "dmPolicy": "pairing",
      "groupPolicy": "open",
      "streaming": true,
      "blockStreaming": true,
      "textChunkLimit": 2000,
      "mediaMaxMb": 30,
      "accounts": {
        "main": {
          "appId": "",
          "appSecret": "",
          "botName": "Sisyphus"
        }
      }
    }
  },
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "lan",
    "auth": {
      "mode": "token"
    },
    "tailscale": {
      "mode": "off",
      "resetOnExit": false
    }
  },
  "tools": {
    "profile": "full",
    "exec": {
      "applyPatch": {
        "enabled": true
      }
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "plugins": {
    "entries": {
      "feishu": {
        "enabled": true
      },
      "qwen-portal-auth": {
        "enabled": true
      }
    }
  },
  "browser": {
    "enabled": true,
    "headless": true,
    "noSandbox": true,
    "defaultProfile": "openclaw",
    "executablePath": "/usr/bin/google-chrome-stable"
  },
  "skills": {
    "install": {
      "nodeManager": "npm"
    }
  }
}
